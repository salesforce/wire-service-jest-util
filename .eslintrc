{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "sourceType": "module"
    },

    "plugins": ["jest", "@typescript-eslint", "import"],
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/eslint-recommended"],
    "rules": {
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
    }
}
